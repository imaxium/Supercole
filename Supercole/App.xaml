             xmlns:local="clr-namespace:Supercole"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="colores\colores.xaml"/>
                <ResourceDictionary Source="estilos\MainStyles.xaml"/>
